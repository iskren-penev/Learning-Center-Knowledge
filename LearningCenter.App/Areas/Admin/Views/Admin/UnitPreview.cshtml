@using LearningCenter.Models.ViewModels.Admin
@model UnitDetailsViewModel
    
@{
    ViewBag.Title = "Preview Unit";
}

<h2 class="heading-centered">@Html.DisplayFor(model => model.Title)</h2>
<hr/>
<h3 class="heading-centered">
    @Html.DisplayNameFor(model => model.CourseName) : @Html.DisplayFor(model => model.CourseName)
</h3>

<div class="col-sm-offset-1 col-sm-10">
    <div class="container jumbotron">
        <iframe src="@Html.DisplayFor(model => model.ContentUrl)" width="920"  height="541" frameborder="0" allowfullscreen="true" mozallowfullscreen="true" webkitallowfullscreen="true"></iframe>
    </div>
</div>
